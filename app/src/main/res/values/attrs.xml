        <attr name="sb_uncheckcircle_radius" format="reference|dimension" />
        <attr name="sb_uncheckcircle_width" format="reference|dimension" />
    </declare-styleable>

    <declare-styleable name="RangeSeekBar">
        <attr name="rsb_gravity">
            <enum name="top" value="0" />
            <enum name="bottom" value="1" />
            <enum name="center" value="2" />
        </attr>
        <attr name="rsb_indicator_arrow_size" format="dimension" />
