<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="@dimen/_32sdp"
        android:background="@drawable/shape_bg_color_white_stroke_primary_corner_16"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tvAdjustNumber33"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/inter_semibold"
            android:paddingHorizontal="@dimen/_16sdp"
            android:paddingVertical="@dimen/_10sdp"
            android:text="33"
            android:textSize="@dimen/_12ssp" />

        <TextView
            android:id="@+id/tvAdjustNumber66"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/inter_semibold"
            android:paddingHorizontal="@dimen/_16sdp"
            android:paddingVertical="@dimen/_10sdp"
            android:text="66"
            android:textSize="@dimen/_12ssp" />

        <TextView
            android:id="@+id/tvAdjustNumber99"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/inter_semibold"
            android:paddingHorizontal="@dimen/_16sdp"
            android:paddingVertical="@dimen/_10sdp"
            android:text="99"
            android:textSize="@dimen/_12ssp" />

        <TextView
            android:id="@+id/tvAdjustNumberCustomize"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/inter_semibold"
            android:paddingHorizontal="@dimen/_16sdp"
            android:paddingVertical="@dimen/_10sdp"
            android:text="@string/string_customize"
            android:textSize="@dimen/_12ssp" />

    </LinearLayout>

</androidx.appcompat.widget.LinearLayoutCompat>